{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "types": ["vite/client"],
    "allowJs": false,
    //    "esModuleInterop": true,
    //    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    "wailsjs",
    "vite.config.ts",
    "uno.config.ts"
  ]
}
